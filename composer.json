{
  "name": "philippejadin/agorakit",
  "description": "Agorakit, groupware for citizen.",
  "keywords": ["groupware", "laravel"],
  "license": "GPL",
  "type": "project",


  "require": {
    "php": ">=7.1.3",
    "barryvdh/laravel-translation-manager": "^0.5.3",
    "beyondcode/laravel-websockets": "^1.1",
    "consoletvs/charts": "5.*",
    "cviebrock/eloquent-sluggable": "4.6",
    "cviebrock/eloquent-taggable": "~3.4",
    "ddeboer/imap": "^1.8",
    "eluceo/ical": "^0.14.0",
    "intervention/image": "^2.4",
    "intervention/imagecache": "2.*",
    "kkomelin/laravel-translatable-string-exporter": "^1.2",
    "kwi/urllinker": "dev-master",
    "laravel/framework": "5.7.*",
    "laravelcollective/html": "5.7.*",
    "nicolaslopezj/searchable": "^1.10",
    "pusher/pusher-php-server": "~3.0",
    "rap2hpoutre/laravel-log-viewer": "0.*",
    "suin/php-rss-writer": "^1.6",
    "tio/laravel": "^1.6",
    "toin0u/geocoder-laravel": "^4.0",
    "venturecraft/revisionable": "1.*",
    "watson/validating": "^3",
    "willdurand/email-reply-parser": "^2.8"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0",
    "barryvdh/laravel-debugbar": "^3",
    "beyondcode/laravel-self-diagnosis": "^1",
    "laravel/browser-kit-testing": "^4.0",
    "laravel/tinker": "~1.0"
  },



  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "app/Helpers/Functions.php",
      "app/Helpers/Filters.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php",
      "tests/BrowserKitTestCase.php"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
